{
  "name": "pollroom-backend",
  "version": "1.0.0",
  "description": "Backend for my realtime voting application",
  "main": "index.js",
  "repository": "https://github.com/mukul-pathania/pollroombackend.git",
  "author": "Mukul Pathania",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "yarn run build",
    "dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "dotenv -e .env.test -- jest",
    "test:dev": "dotenv -e .env.test -- jest --watch --silent --runInBand --verbose",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:test": "dotenv -e .env.test -- yarn prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "2.27.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.3.2",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.3",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "axios": "^0.21.1",
    "concurrently": "^6.2.0",
    "docker-compose": "^0.23.13",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "is-port-reachable": "^3.0.0",
    "jest": "^27.0.6",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prisma": "^3.0.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  }
}
